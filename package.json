{
  "name": "routines-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npm run orm:generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "global:settings": "ts-node --files src/scripts/global-routine-settings.ts",
    "global:settings:prod": "node dist/scripts/global-routine-settings.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "orm:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest.e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@prisma/client": "^4.7.1",
    "cron-parser": "^4.6.0",
    "dayjs": "^1.11.6",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.0",
    "lodash": "^4.17.21",
    "nats": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.1.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.3.1",
    "prettier": "^2.8.2",
    "prisma": "^4.5.0",
    "source-map-support": "^0.5.21",
    "testcontainers": "^9.0.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": ["ts-jest", {
          "compiler": "ttypescript"
      }]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}